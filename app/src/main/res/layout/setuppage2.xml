<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/set_up_page_1"
    android:visibility="visible">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="84dp"
        android:fontFamily="@font/grgaref"

        android:gravity="center_horizontal"
        android:text="Step 2/4"
        android:textColor="@color/Theme"
        android:textSize="45dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="156dp"
        android:fontFamily="@font/grgaref"
        android:gravity="center_horizontal"
        android:text="Configure your vehicle"
        android:textColor="@color/Theme"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:gravity="center_horizontal"
        android:text="Distance for oil change (km)"
        android:textColor="@color/Theme_second"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintHorizontal_bias="0.652"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.379" />

    <EditText
        android:id="@+id/editTextOilChangeTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="152dp"
        android:background="@drawable/text_field"
        android:textColorHint="@color/gray_dark"
        android:drawableTint="@color/Theme_second"
        android:ems="10"
        android:hint="days"
        android:inputType="number"

        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="@color/Theme_second"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.018"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:text="Time for oil change (days)"
        android:textColor="@color/Theme_second"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editTextOilChangeTime"
        android:layout_marginStart="47dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextOilChangeDis"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/next_btn_2"
        android:layout_width="65dp"
        android:layout_height="63dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="26dp"
        android:background="@drawable/next"
        android:contentDescription="@android:string/untitled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <Button
        android:id="@+id/return_btn_2"
        android:layout_width="62dp"
        android:layout_height="61dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/transparent"
        android:contentDescription=""
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"

        android:layout_marginStart="47dp"
        android:layout_marginBottom="9dp"
        android:text="Distance for maintenance (km)"
        android:textAlignment="textStart"
        android:textColor="@color/Theme_second"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editTextMaintenanceDis"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextOilChangeTime"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editTextMaintenanceDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="188dp"
        android:layout_marginEnd="152dp"
        android:textColorHint="@color/gray_dark"
        android:background="@drawable/text_field"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="km"
        android:inputType="number"
        android:minHeight="48dp"

        android:paddingLeft="10dp"
        android:textColor="@color/Theme_second"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.018"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/editTextOilChangeDis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="152dp"
        android:background="@drawable/text_field"
        android:drawablePadding="10dp"
        android:ems="10"
        android:textColorHint="@color/gray_dark"
        android:hint="km"
        android:inputType="number"
        android:minHeight="48dp"

        android:paddingLeft="10dp"
        android:textColor="@color/Theme_second"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.018"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/editTextMaintenanceTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginTop="280dp"
        android:layout_marginEnd="152dp"
        android:background="@drawable/text_field"
        android:textColorHint="@color/gray_dark"
        android:drawableTint="@color/Theme_second"
        android:ems="10"
        android:hint="days"
        android:inputType="number"

        android:minHeight="48dp"
        android:paddingLeft="10dp"
        android:textColor="@color/Theme_second"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.018"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:layout_marginStart="47dp"
        android:layout_marginBottom="10dp"

        android:text="Time for maintenance (days)"
        android:textColor="@color/Theme_second"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editTextMaintenanceTime"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextMaintenanceDis"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="222dp"

        android:layout_height="54dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7">

        <androidx.appcompat.widget.AppCompatSpinner
            android:id="@+id/autoCompleteTextView4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"

            android:outlineSpotShadowColor="@color/Theme"
            android:theme="@style/Platform.MaterialComponents.Light"
            android:popupBackground="@drawable/text_field"
            android:spinnerMode="dropdown"
            android:text="@android:string/defaultMsisdnAlphaTag" />


    </com.google.android.material.textfield.TextInputLayout>


</androidx.constraintlayout.widget.ConstraintLayout>